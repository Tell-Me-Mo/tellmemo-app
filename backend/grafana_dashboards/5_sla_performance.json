{
  "title": "\ud83c\udfaf SLA & Performance",
  "tags": [
    "tellmemo",
    "sla",
    "performance"
  ],
  "timezone": "browser",
  "refresh": "30s",
  "panels": [
    {
      "title": "SLA Compliance Rate",
      "type": "gauge",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "% of requests meeting SLA (target: >95%)",
      "targets": [
        {
          "expr": "avg(business_sla_compliance_rate) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "SLA Violations (Today)",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "Total SLA violations in last 24 hours",
      "targets": [
        {
          "expr": "sum(increase(business_sla_violations[24h]))",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Error Budget Remaining",
      "type": "gauge",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "Remaining error budget (target: >10%)",
      "targets": [
        {
          "expr": "avg(business_sla_error_budget_remaining) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "System Availability",
      "type": "gauge",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "Overall system availability (target: 99.9%)",
      "targets": [
        {
          "expr": "avg(business_sla_availability) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "SLA Compliance Over Time",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "description": "SLA compliance rate trend",
      "targets": [
        {
          "expr": "avg(business_sla_compliance_rate) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "SLA Violations by Operation",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "description": "SLA violations per operation type",
      "targets": [
        {
          "expr": "sum(rate(business_sla_violations[5m])) by (operation) * 300",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "RAG Query Response Time (P95)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "description": "95th percentile RAG query response time (SLA: <2000ms)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(business_sla_compliance_rate_bucket[5m])) by (le))",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "Error Budget Trend",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "description": "Error budget remaining over time",
      "targets": [
        {
          "expr": "avg(business_sla_error_budget_remaining) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    }
  ],
  "schemaVersion": 36
}