{
  "title": "\ud83d\ude80 Time-to-Value",
  "tags": [
    "tellmemo",
    "onboarding",
    "activation"
  ],
  "timezone": "browser",
  "refresh": "5m",
  "panels": [
    {
      "title": "Avg Time to First Query",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "Median time from signup to first query (minutes)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(business_users_time_to_first_query_bucket[24h])) by (le)) / 60",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "P95 Time to First Query",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "95th percentile time to first query (target: <5 min)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(business_users_time_to_first_query_bucket[24h])) by (le)) / 60",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "First Query Success Rate",
      "type": "gauge",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "% of first queries that succeeded (target: >70%)",
      "targets": [
        {
          "expr": "sum(increase(business_users_first_query_success{success=\"true\"}[24h])) / sum(increase(business_users_first_query_success[24h])) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 70,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "Avg Queries Until Success",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "description": "Average attempts before successful query (target: <3)",
      "targets": [
        {
          "expr": "avg(business_users_queries_until_success)",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "horizontal",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Time to First Query Distribution",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "description": "Median time to first query over time (minutes)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(business_users_time_to_first_query_bucket[1h])) by (le)) / 60",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "First Query Success Rate Trend",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "description": "First query success rate over time",
      "targets": [
        {
          "expr": "sum(rate(business_users_first_query_success{success=\"true\"}[5m])) / sum(rate(business_users_first_query_success[5m])) * 100",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "Time to First Project",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "description": "Median time from signup to first project creation",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(business_users_time_to_first_project_bucket[24h])) by (le)) / 60",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "title": "Queries Until Success Distribution",
      "type": "histogram",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "description": "Distribution of attempts before successful query",
      "targets": [
        {
          "expr": "sum(business_users_queries_until_success) by (le)",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    }
  ],
  "schemaVersion": 36
}